import java.util.Scanner;
public class URI1012 {
 
public static void main(String args[]) {
	Scanner teclado;
	teclado = new Scanner(System.in);
	
	
	double A,B, C, triangulo, circulo, trapezio, quadrado, retangulo;
	
	//System.out.print("Digite um valor A:");
	A = teclado.nextDouble();
	
    //System.out.print("Digite o segundo valor B:");
    B = teclado.nextDouble();
    
    //System.out.print("Digite o terceiro valor C");
    C = teclado.nextDouble();
    
	triangulo = (A*C)/2;
    circulo = 3.14159*C*C;
    trapezio = (A+B)*C/2;
    quadrado = B*B;
    retangulo = A*B;
    
    
	
	
 
  System.out.print("triangulo:" +triangulo);
  System.out.print("circulo:"   +circulo);
  System.out.print("trapezio:"  +trapezio);
  System.out.print("quadrado:"  +quadrado);
  System.out.print("retangulo:" +retangulo);

}
	

}
