import java.util.Scanner;
public class URI1012 {
 
public static void main(String args[]) {
	Scanner teclado;
	teclado = new Scanner(System.in);
	
	
	double A,B, C, triangulo, circulo, trapezio, quadrado, retangulo;
	
	
	A = teclado.nextDouble();
	
    
    B = teclado.nextDouble();
    
   
    C = teclado.nextDouble();
    
	triangulo = (A*C)/2;
    circulo = 3.14159*C*C;
    trapezio = (A+B)*C/2;
    quadrado = B*B;
    retangulo = A*B;
    
    
	
	
 
  System.out.print("TRIANGULO: %.3f\n" +triangulo);
  System.out.print("CIRCULO: %.3f\n"   +circulo);
  System.out.print("TRAPEZIO: %.3f\n"  +trapezio);
  System.out.print("QUADRADO: %.3f\n"  +quadrado);
  System.out.print("RETANGULO: %.3f\n" +retangulo);
  
  teclado.close();

}
	

}
